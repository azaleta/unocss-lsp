{
  "publisher": "azaleta",
  "name": "vscode-unocss",
  "version": "0.0.1",
  "description": "",
  "author": "azaleta <24407500@qq.com>",
  "license": "MIT",
  "keywords": [
    "unocss",
    "Unocss",
    "css",
    "intellisense",
    "autocomplete",
    "vscode"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "engines": {
    "vscode": "^1.68.0"
  },
  "activationEvents": [
    "onLanguage:plaintext"
  ],
  "contributes": {
    "languages": [
      {
        "id": "unocss",
        "aliases": [
          "Unocss"
        ]
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Example configuration",
      "properties": {
        "unocss-lsp.trace.server": {
          "scope": "window",
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "verbose",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    }
  },
  "scripts": {
    "build": "npm run clean && tsup",
    "dev": "tsup --watch src",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@azaleta/unocss-language-server": "^0.0.3",
    "vscode-languageclient": "^8.0.1",
    "vscode-languageserver": "^8.0.1"
  },
  "devDependencies": {
    "@types/vscode": "^1.68.1",
    "vsce": "^2.9.2"
  }
}
